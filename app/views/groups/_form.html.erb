  <%= form_with model: group do |f| %>
  <div class="form-group">
    <%= f.label :name, "グループ名" %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :introduce, "紹介文" %>
    <%= f.text_area :introduce, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :group_image %>
    <%= f.file_field :group_image, class: "form-control-file", accept: 'image/*' %>
  </div>
  <div class="form-group">
    <%= f.submit "送信", class: "btn btn-md btn-info" %>
  </div>
<% end %>